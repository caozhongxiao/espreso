# ESPRESO Configuration File


INPUT                  WORKBENCH;
PHYSICS   ADVECTION_DIFFUSION_3D;

WORKBENCH {
  PATH   workbench.dat;
  DOMAINS            8;
}

ADVECTION_DIFFUSION_3D {
  PHYSICS_SOLVER {
    LOAD_STEPS   1;

    LOAD_STEPS_SETTINGS {
      1 {
        TYPE   STEADY_STATE;
        MODE         LINEAR;
        SOLVER         FETI;

        FETI {
          METHOD         HYBRID_FETI;
          PRECONDITIONER   DIRICHLET;
          SOLVER                 PCG;
          REGULARIZATION    ANALYTIC;
        }
      }
    }
  }

}
