# ESPRESO Configuration File


INPUT            WORKBENCH;
PHYSICS   HEAT_TRANSFER_3D;

WORKBENCH {
  PATH   workbench.dat;
  DOMAINS            2;
}

HEAT_TRANSFER_3D {
  LOAD_STEPS   1;


  LOAD_STEPS_SETTINGS {
    1 {
      TYPE   STEADY_STATE;
      MODE         LINEAR;
      SOLVER         FETI;

      FETI {
        METHOD         HYBRID_FETI;
        PRECONDITIONER      LUMPED;
        SOLVER                 PCG;
        REGULARIZATION   ALGEBRAIC;
      }
    }
  }
}
