INPUT WORKBENCH;
PHYSICS HEAT_TRANSFER_2D;

WORKBENCH {
  PATH    workbench.dat;
  DOMAINS 4;
}

HEAT_TRANSFER_2D {

  
    LOAD_STEPS          1;

    LOAD_STEPS_SETTINGS {
      1 {
        TYPE        STEADY_STATE;
        MODE              LINEAR;
        SOLVER FETI;

        FETI {
          METHOD           TOTAL_FETI;
          ITERATIVE_SOLVER PCG;
          PRECONDITIONER    DIRICHLET;
          REGULARIZATION  ALGEBRAIC;
        }
      
    }
  }

}
